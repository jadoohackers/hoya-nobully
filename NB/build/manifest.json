{
  "manifest_version": 2,
  "name": "No Bullying on Twitter",
  "version": "2.4.4",
  "description": "__MSG_description__",
  "update_url": "http://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "js": [
        "data/js/filter.js"
      ],
      "matches": [
        "http://twitter.com/*",
        "https://twitter.com/*"
      ]
    }
  ],
  "icons": {
    "128": "data/img/icon.png"
  },
  "default_locale": "en"
}